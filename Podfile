#  MARK: Linter
  pod 'SwiftLint', '~> 0.39'
#  MARK: GRPC
  pod 'gRPC-Swift', '1.0.0-alpha.12'
#  MARK: Sentry
  pod 'Sentry', :git => 'https://github.com/getsentry/sentry-cocoa.git', :tag => '5.1.5'


end
